{
  "name": "sql-query-buddy",
  "version": "1.0.0",
  "description": "A conversational AI agent that converts natural language questions into SQL queries, executes them, and provides AI-driven insights using LangChain, RAG, and Vector Databases",
  "main": "index.js",
  "scripts": {
    "start": "node index.js --app-name=SQL_Query_Buddy",
    "dev": "nodemon index.js --app-name=SQL_Query_Buddy",
    "dev:watch": "nodemon --watch . --ext js,json --ignore public/ index.js --app-name=SQL_Query_Buddy",
    "build": "echo \"Build process: Assets are served directly. For production, consider using a CDN for external libraries.\"",
    "build-db": "node scripts/build-database.js",
    "build-db-from-sql": "node scripts/build-database-from-sql.js",
    "rebuild-fix-db": "node scripts/rebuild-and-fix-database.js",
    "optimize-db": "node scripts/optimize-database.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:baseline": "node ../test-baseline-requirements.js",
    "test:baseline:comprehensive": "node ../test-baseline-requirements-comprehensive.js"
  },
  "keywords": [
    "ai",
    "langchain",
    "sql",
    "natural-language",
    "rag",
    "vector-database",
    "faiss",
    "openai",
    "chatbot",
    "database-query"
  ],
  "author": {
    "name": "jorgenyc",
    "url": "https://www.linkedin.com/in/jorgenyc/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jorgenyc/sql-query-buddy"
  },
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "@huggingface/inference": "^4.13.2",
    "@langchain/cohere": "^1.0.0",
    "@langchain/community": "^1.0.0",
    "@langchain/core": "^1.0.3",
    "@langchain/openai": "^0.0.14",
    "ai21": "^1.3.0",
    "cohere-ai": "^7.19.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.1",
    "faiss-node": "^0.5.1",
    "helmet": "^7.2.0",
    "langchain": "^0.1.3",
    "openai": "^6.8.1",
    "p-retry": "^7.1.0",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  },
  "overrides": {
    "rimraf": "^4.0.0",
    "glob": "^9.0.0",
    "node-domexception": "^1.0.0",
    "npmlog": "^7.0.0",
    "are-we-there-yet": "^5.0.0",
    "gauge": "^5.0.0"
  }
}
